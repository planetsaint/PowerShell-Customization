{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": "\uE0B6",
          "foreground": "#f5f5f5",
          "background": "#217EA1",
          "properties": {
            "prefix": "\uE5FF ",
            "postfix": "",
            "style": "folder"
          }
        },
        {
         "type": "executiontime",
         "style": "diamond",
         "trailing_diamond": "\uE0B4",
         "leading_diamond": " \uE0B6",
         "foreground": "#2ac0f6",
         "background": "#18212f",
         "properties": {
         "threshold": 0,
         "prefix": "\uE5FF ",
         "postfix": ""
         }
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#217EA1",
          "properties": {
            "prefix": "",
            "postfix": "",
            "template": "{{ .HEAD }}",
            "branch_icon": ""
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#97BEC4",
          "properties": {
            "prefix": "\uE235",
            "postfix": "",
            "display_version": true
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#58babf",
          "properties": {
            "prefix": "\uFCD1",
            "postfix": "",
            "display_version": true
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#7cd9b4",
          "properties": {
            "prefix": "\uE718",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#f36374",
          "properties": {
            "prefix": "\uE791",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#663399",
          "properties": {
            "prefix": "\uE738",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "julia",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#18212F",
          "properties": {
            "prefix": "\uE624",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#f5f5f5",
          "background": "#18212F",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#b6d7a8{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ed9600{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "properties": {
            "discharging_icon": "\u21E3 ",
            "charging_icon": "\u21E1 ",
            "charged_icon": "\u25CF ",
            "prefix": "",
            "postfix": ""
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "trailing_diamond": "\uE0B4 ",
          "leading_diamond": "\uE0B6",
          "foreground": "#f5f5f5",
          "background": "#217EA1",
          "properties": {
            "postfix": " \u276F",
            "prefix": "",
            "template": "{{ .UserName }}"
          }
        }
      ]
    }
  ]
}
